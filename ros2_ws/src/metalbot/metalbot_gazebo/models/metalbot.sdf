          <limit>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </limit>
          <suspension>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </suspension>
        </ode>
      </physics>
    </joint>

    <!-- REAR RIGHT WHEEL HINGE -->
    <joint name='rear_right_wheel_hinge' type='revolute'>
      <parent>base_link</parent>
      <child>rear_right_wheel</child>
      <pose>0 0 0 0 -0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
          <effort>-1</effort>
          <velocity>-1</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </limit>
          <suspension>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </suspension>
        </ode>
      </physics>
    </joint>
    <static>0</static>
    <allow_auto_disable>1</allow_auto_disable>
   
    <!-- CAMERA JOINT -->
    <joint name='camera_joint' type='fixed'>
      <parent>base_link</parent>
      <child>camera_link</child>
      <pose>0 0 1.45 0 -0 0</pose>
      <physics>
        <ode>
          <limit>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </limit>
          <suspension>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </suspension>
        </ode>
      </physics>
    </joint>

    <!-- MONITOR JOINT -->
    <joint name='monitor_joint' type='fixed'>
      <parent>base_link</parent>
      <child>monitor_link</child>
      <pose>0 0 0 0 -0 0</pose>
      <physics>
        <ode>
          <limit>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </limit>
          <suspension>
            <cfm>0</cfm>
            <erp>0.2</erp>
          </suspension>
        </ode>
      </physics>
    </joint>

    <!-- CONTROLLER PLUGIN -->
    <plugin name='skid_steer_drive' filename='libgazebo_ros_diff_drive.so'>
      <ros/>
      <update_rate>20</update_rate>
      <num_wheel_pairs>2</num_wheel_pairs>
      <!-- <left_joint>front_left_wheel_hinge</left_joint>
      <right_joint>front_right_wheel_hinge</right_joint> -->
      <left_joint>rear_left_wheel_hinge</left_joint>
      <right_joint>rear_right_wheel_hinge</right_joint>
      <left_joint>front_left_wheel_hinge</left_joint>
      <right_joint>front_right_wheel_hinge</right_joint>
      <wheel_separation>0.4</wheel_separation>
      <wheel_separation>0.4</wheel_separation>
      <wheel_diameter>0.2</wheel_diameter>
      <wheel_diameter>0.2</wheel_diameter>
      <max_wheel_torque>0.5</max_wheel_torque>
      <max_wheel_acceleration>2.5</max_wheel_acceleration>
      <publish_odom>1</publish_odom>
      <publish_odom_tf>1</publish_odom_tf>
      <publish_wheel_tf>1</publish_wheel_tf>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>
    </plugin>
  </model>
</sdf>
