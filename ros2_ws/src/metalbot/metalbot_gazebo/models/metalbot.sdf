            <argument>~/out:=imu</argument>
          </ros>
          <frame_name>imu</frame_name>
        </plugin>
        <pose>0 0 0 0 -0 0</pose>
        <imu>
          <angular_velocity>
            <x>
              <noise type='none'/>
            </x>
            <y>
              <noise type='none'/>
            </y>
            <z>
              <noise type='none'/>
            </z>
          </angular_velocity>
          <linear_acceleration>
